<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqBill" 
    table="turq_bills"
>

    <id
        name="billsId"
        type="int"
        column="bills_id"
    >
        <generator class="sequence">
            <param name="sequence">seq_bills</param>
        </generator>
    </id>

    <property
        name="billsType"
        type="int"
        column="bills_type"
        not-null="true"
        length="4"
    />
    <property
        name="billDocumentNo"
        type="java.lang.String"
        column="bill_document_no"
        not-null="true"
        length="50"
    />
    <property
        name="billsDate"
        type="java.sql.Date"
        column="bills_date"
        not-null="true"
        length="4"
    />
    <property
        name="billsDefinition"
        type="java.lang.String"
        column="bills_definition"
        not-null="true"
        length="250"
    />
    <property
        name="billsPrinted"
        type="short"
        column="bills_printed"
        not-null="true"
        length="2"
    />
    <property
        name="billsDiscountRate"
        type="int"
        column="bills_discount_rate"
        not-null="true"
        length="4"
    />
    <property
        name="billsDiscountAmount"
        type="java.math.BigDecimal"
        column="bills_discount_amount"
        not-null="true"
        length="65535"
    />
    <property
        name="billsCharges"
        type="java.math.BigDecimal"
        column="bills_charges"
        not-null="true"
        length="65535"
    />
    <property
        name="billsVat"
        type="int"
        column="bills_vat"
        not-null="true"
        length="4"
    />
    <property
        name="billsVatAmount"
        type="java.math.BigDecimal"
        column="bills_vat_amount"
        not-null="true"
        length="65535"
    />
    <property
        name="billsTotalAmount"
        type="java.math.BigDecimal"
        column="bills_total_amount"
        not-null="true"
        length="65535"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="created_by"
        not-null="true"
        length="50"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TurqCompany -->
    <many-to-one
        name="turqCompany"
        class="com.turquaz.engine.dal.TurqCompany"
        not-null="true"
    >
        <column name="companies_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to TurqBillInGroup -->
    <set
        name="turqBillInGroups"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="bills_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqBillInGroup"
        />
    </set>
    <!-- bi-directional one-to-many association to TurqConsignment -->
    <set
        name="turqConsignments"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="bills_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqConsignment"
        />
    </set>
    <!-- bi-directional one-to-many association to TurqOrder -->
    <set
        name="turqOrders"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="bills_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqOrder"
        />
    </set>
    <!-- bi-directional many-to-one association to TurqCurrentCard -->
    <many-to-one
        name="turqCurrentCard"
        class="com.turquaz.engine.dal.TurqCurrentCard"
        not-null="true"
    >
        <column name="current_cards_id" />
    </many-to-one>

</class>
</hibernate-mapping>
