<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqInventoryUnit" 
    table="turq_inventory_units"
>

    <id
        name="inventoryUnitsId"
        type="int"
        column="inventory_units_id"
    >
        <generator class="sequence">
            <param name="sequence">seq_inventory_units</param>
        </generator>
    </id>

    <property
        name="unitsName"
        type="java.lang.String"
        column="units_name"
        not-null="true"
        length="50"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="created_by"
        not-null="true"
        length="50"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TurqCompany -->
    <many-to-one
        name="turqCompany"
        class="com.turquaz.engine.dal.TurqCompany"
        not-null="true"
    >
        <column name="companies_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to TurqInventoryTransaction -->
    <set
        name="turqInventoryTransactions"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="inventory_units_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqInventoryTransaction"
        />
    </set>
    <!-- bi-directional one-to-many association to TurqInventoryCardUnit -->
    <set
        name="turqInventoryCardUnits"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="inventory_units_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqInventoryCardUnit"
        />
    </set>

</class>
</hibernate-mapping>
