<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqCurrentTransaction" 
    table="turq_current_transactions"
>

    <id
        name="currentTransactionsId"
        type="int"
        column="current_transactions_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="transactionsDate"
        type="java.sql.Date"
        column="transactions_date"
        not-null="true"
        length="4"
    />
    <property
        name="transactionsDocumentNo"
        type="java.lang.String"
        column="transactions_document_no"
        not-null="true"
    />
    <property
        name="currentTransactionTypesId"
        type="int"
        column="current_transaction_types_id"
        not-null="true"
        length="4"
    />
    <property
        name="transactionsTotalAmount"
        type="java.math.BigDecimal"
        column="transactions_total_amount"
        not-null="true"
        length="65535"
    />
    <property
        name="transactionsTotalDiscount"
        type="java.math.BigDecimal"
        column="transactions_total_discount"
        not-null="true"
        length="65535"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="created_by"
        not-null="true"
        length="50"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to TurqCurrentTransactionBill -->
    <set
        name="turqCurrentTransactionBillsByCurrentTransactionsIdClose"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="current_transactions_id_close" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqCurrentTransactionBill"
        />
    </set>
    <!-- bi-directional one-to-many association to TurqCurrentTransactionBill -->
    <set
        name="turqCurrentTransactionBillsByCurrentTransactionsIdOpen"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="current_transactions_id_open" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqCurrentTransactionBill"
        />
    </set>
    <!-- bi-directional many-to-one association to TurqCurrency -->
    <many-to-one
        name="turqCurrency"
        class="com.turquaz.engine.dal.TurqCurrency"
        not-null="true"
    >
        <column name="currencies_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to TurqCurrentCard -->
    <many-to-one
        name="turqCurrentCard"
        class="com.turquaz.engine.dal.TurqCurrentCard"
        not-null="true"
    >
        <column name="current_cards_id" />
    </many-to-one>

</class>
</hibernate-mapping>
