<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqInventoryWarehous" 
    table="turq_inventory_warehouses"
>

    <id
        name="inventoryWarehousesId"
        type="int"
        column="inventory_warehouses_id"
    >
         <generator class="increment" />
    </id>

    <property
        name="warehousesName"
        type="java.lang.String"
        column="warehouses_name"
        not-null="true"
        length="50"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="created_by"
        not-null="true"
        length="50"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />
    <property
        name="warehousesAddress"
        type="java.lang.String"
        column="warehouses_address"
        length="250"
    />
    <property
        name="warehousesDescription"
        type="java.lang.String"
        column="warehouses_description"
        length="250"
    />
    <property
        name="warehousesCity"
        type="java.lang.String"
        column="warehouses_city"
        length="25"
    />
    <property
        name="warehousesTelephone"
        type="java.lang.String"
        column="warehouses_telephone"
        length="25"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TurqCompany -->
    <many-to-one
        name="turqCompany"
        class="com.turquaz.engine.dal.TurqCompany"
        not-null="true"
    >
        <column name="companies_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to TurqInventoryTransaction -->
    <set
        name="turqInventoryTransactions"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="inventory_warehouses_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqInventoryTransaction"
        />
    </set>

</class>
</hibernate-mapping>
