        column="card_minimum_amount"
        not-null="true"
        length="4"
    />
    <property
        name="cardMaximumAmount"
        type="int"
        column="card_maximum_amount"
        not-null="true"
        length="4"
    />
    <property
        name="cardVat"
        type="int"
        column="card_vat"
        not-null="true"
        length="4"
    />
    <property
        name="cardDiscount"
        type="int"
        column="card_discount"
        not-null="true"
        length="4"
    />
    <property
        name="cardSpecialVat"
