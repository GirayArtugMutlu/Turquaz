<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqCurrentAccountingAccount" 
    table="turq_current_accounting_accounts"
>

    <id
        name="currentAccountingAccountsId"
        type="int"
        column="current_accounting_accounts_id"
    >
        <generator class="native" />
    </id>

    <property
        name="createdBy"
        type="java.lang.String"
        column="created_by"
        not-null="true"
        length="50"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TurqCurrentAccountingType -->
    <many-to-one
        name="turqCurrentAccountingType"
        class="com.turquaz.engine.dal.TurqCurrentAccountingType"
        not-null="true"
    >
        <column name="current_accounting_types_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to TurqAccountingAccount -->
    <many-to-one
        name="turqAccountingAccount"
        class="com.turquaz.engine.dal.TurqAccountingAccount"
        not-null="true"
    >
        <column name="accounting_accounts_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to TurqCurrentCard -->
    <many-to-one
        name="turqCurrentCard"
        class="com.turquaz.engine.dal.TurqCurrentCard"
        not-null="true"
    >
        <column name="current_cards_id" />
    </many-to-one>

</class>
</hibernate-mapping>
