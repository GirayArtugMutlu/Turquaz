    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqTradebillTransactionType" 
    table="turq_tradebill_transaction_types"
>

    <id
        name="tradebillTransactionTypesId"
        type="int"
        column="tradebill_transaction_types_id"
    >
        <generator class="sequence">
            <param name="sequence">seq_tradebill_transaction_types</param>
        </generator>
    </id>

    <property
        name="transactionTypesName"
        type="java.lang.String"
        column="transaction_types_name"
        not-null="true"
        length="50"
    />
    <property
        name="transactionTypesParent"
        type="short"
        column="transaction_types_parent"
        not-null="true"
        length="2"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="createdBy"
        type="java.sql.Date"
        column="created_by"
        not-null="true"
        length="4"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TurqAccountingAccount -->
    <many-to-one
        name="turqAccountingAccount"
        class="com.turquaz.engine.dal.TurqAccountingAccount"
        not-null="true"
    >
        <column name="accounting_accounts_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to TurqTradebillRoll -->
    <set
        name="turqTradebillRolls"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="tradebill_transaction_types_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqTradebillRoll"
        />
    </set>

</class>
</hibernate-mapping>
