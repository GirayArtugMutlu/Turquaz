    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to TurqChequeChequeInRoll -->
    <set
        name="turqChequeChequeInRolls"
        lazy="true"
        inverse="true"
