<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqBankSecondaryAccount" 
    table="turq_bank_secondary_accounts"
>

    <id
        name="bankSecondaryAccountsId"
        type="int"
        column="bank_secondary_accounts_id"
    >
      <generator class="increment" />
    </id>

    <property
        name="accountName"
        type="java.lang.String"
        column="account_name"
        not-null="true"
        length="50"
    />
    <property
        name="accountCode"
        type="java.lang.String"
        column="account_code"
        not-null="true"
        length="5"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="createdBy"
        type="java.lang.String"
        column="created_by"
        not-null="true"
        length="50"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TurqCompany -->
    <many-to-one
        name="turqCompany"
        class="com.turquaz.engine.dal.TurqCompany"
        not-null="true"
    >
        <column name="companies_id" />
    </many-to-one>
    <!-- bi-directional one-to-many association to TurqBankCardsSecondaryAccount -->
    <set
        name="turqBankCardsSecondaryAccounts"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="bank_secondary_accounts_id" />
        </key>
        <one-to-many 
            class="com.turquaz.engine.dal.TurqBankCardsSecondaryAccount"
        />
    </set>

</class>
</hibernate-mapping>
