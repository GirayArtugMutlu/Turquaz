<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.turquaz.engine.dal.TurqCurrentTransactionBill" 
    table="turq_current_transaction_bill"
>

    <id
        name="currentTransactionBillId"
        type="int"
        column="current_transaction_bill_id"
    >
        <generator class="increment" />
    </id>

    <property
        name="createdBy"
        type="java.lang.String"
        column="created_by"
        not-null="true"
        length="50"
    />
    <property
        name="creationDate"
        type="java.sql.Date"
        column="creation_date"
        not-null="true"
        length="4"
    />
    <property
        name="updatedBy"
        type="java.lang.String"
        column="updated_by"
        not-null="true"
        length="50"
    />
    <property
        name="lastModified"
        type="java.sql.Date"
        column="last_modified"
        not-null="true"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TurqCurrentTransaction -->
    <many-to-one
        name="turqCurrentTransactionByCurrentTransactionsIdOpen"
        class="com.turquaz.engine.dal.TurqCurrentTransaction"
        not-null="true"
    >
        <column name="current_transactions_id_open" />
    </many-to-one>
    <!-- bi-directional many-to-one association to TurqCurrentTransaction -->
    <many-to-one
        name="turqCurrentTransactionByCurrentTransactionsIdClose"
        class="com.turquaz.engine.dal.TurqCurrentTransaction"
        not-null="true"
    >
        <column name="current_transactions_id_close" />
    </many-to-one>

</class>
</hibernate-mapping>
